{
  // Skills Adapter Test Scenario
  "turbo-ai-rules.adapters": {
    "cursor": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "copilot": {
      "enabled": true,
      "sortBy": "id",
      "sortOrder": "desc"
    },
    "continue": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "windsurf": {
      "enabled": true,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "cline": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "roo-cline": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "aider": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "bolt": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "qodo-gen": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    }
  },
  "turbo-ai-rules.sync.conflictStrategy": "priority",
  "turbo-ai-rules.sync.auto": false,
  "turbo-ai-rules.sync.onStartup": false,

  // Pre-configured skills rule source
  "turbo-ai-rules.sources": [
    {
      "id": "turbo-skills-source",
      "name": "Turbo AI Skills",
      "gitUrl": "https://github.com/ygqygq2/ai-rules.git",
      "branch": "main",
      "subPath": "1300-skills/",
      "enabled": true,
      "authentication": {
        "type": "none"
      }
    }
  ],

  // Custom Skills Adapter Configuration
  "turbo-ai-rules.adapters.custom": [
    {
      "id": "dev-skills",
      "name": "Development Skills",
      "enabled": true,
      "outputPath": ".skills/",
      "outputType": "directory",
      "fileExtensions": [
        ".md"
      ],
      "organizeBySource": false,
      "generateIndex": true,
      "indexFileName": "index.md",
      "isRuleType": false
    },
    {
      "id": "tt",
      "name": "T",
      "enabled": true,
      "outputPath": ".testrules",
      "outputType": "file",
      "fileExtensions": [],
      "organizeBySource": false,
      "generateIndex": true,
      "indexFileName": "index.md",
      "isRuleType": false,
      "fileTemplate": "{{rules}}",
      "sortBy": "id",
      "sortOrder": "desc"
    }
  ]
}
