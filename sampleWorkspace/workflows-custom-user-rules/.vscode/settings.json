{
  "turbo-ai-rules.sync.onStartup": false,
  "turbo-ai-rules.sources": [
    {
      "id": "ai-rules",
      "name": "AI Rules",
      "gitUrl": "https://github.com/ygqygq2/ai-rules.git",
      "branch": "main",
      "enabled": true,
      "authentication": {
        "type": "none"
      }
    }
  ],
  "turbo-ai-rules.adapters": {
    "cursor": {
      "enabled": true,
      "enableUserRules": true,
      "sortBy": "priority",
      "sortOrder": "asc"
    }
  },
  "turbo-ai-rules.userRules": {
    "directory": "ai-rules",
    "markers": {
      "begin": "<!-- USER-RULES:BEGIN -->",
      "end": "<!-- USER-RULES:END -->"
    }
  }
}
