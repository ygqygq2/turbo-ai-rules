{
  // 自定义适配器测试场景
  "turbo-ai-rules.sync.conflictStrategy": "priority",
  "turbo-ai-rules.sync.auto": false,
  "turbo-ai-rules.sync.onStartup": false,

  // 预配置测试用规则源
  "turbo-ai-rules.sources": [
    {
      "id": "ai-rules-7008d805",
      "name": "Test Custom Adapter Rules",
      "gitUrl": "https://github.com/ygqygq2/ai-rules.git",
      "branch": "main",
      "subPath": "/",
      "enabled": true,
      "authentication": {
        "type": "none"
      }
    }
  ],

  // 自定义适配器配置（测试用，默认禁用）
  "turbo-ai-rules.adapters.custom": [
    {
      "id": "test-preserve-true",
      "name": "Test Preserve Structure",
      "enabled": false,
      "autoUpdate": false,
      "outputPath": "test-preserve-structure/true",
      "outputType": "directory",
      "preserveDirectoryStructure": true
    },
    {
      "id": "test-preserve-false",
      "name": "Test Flatten Structure",
      "enabled": false,
      "autoUpdate": false,
      "outputPath": "test-preserve-structure/false",
      "outputType": "directory",
      "preserveDirectoryStructure": false
    },
    {
      "id": "test-preserve-index",
      "name": "Test Index Paths",
      "enabled": false,
      "autoUpdate": false,
      "outputPath": "test-preserve-structure/index",
      "outputType": "directory",
      "preserveDirectoryStructure": true,
      "generateIndex": true
    }
  ]
}
