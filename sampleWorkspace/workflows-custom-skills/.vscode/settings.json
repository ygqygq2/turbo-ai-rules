{
  "turbo-ai-rules.sync.onStartup": false,
  "turbo-ai-rules.sources": [
    {
      "id": "ai-rules",
      "name": "AI Rules",
      "gitUrl": "https://github.com/ygqygq2/ai-rules.git",
      "branch": "main",
      "enabled": true,
      "authentication": {
        "type": "none"
      }
    }
  ],
  "turbo-ai-rules.adapters": {
    "cursor": {
      "enabled": true,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "windsurf": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "copilot": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "continue": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "cline": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "roo-cline": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "aider": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "bolt": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    },
    "qodo-gen": {
      "enabled": false,
      "sortBy": "priority",
      "sortOrder": "asc"
    }
  },
  "turbo-ai-rules.adapters.custom": [
    {
      "id": "skills",
      "name": "Custom Skills Adapter",
      "outputPath": ".skills",
      "outputType": "directory",
      "isRuleType": false,
      "preserveStructure": true,
      "enabled": true
    },
    {
      "id": "cursor",
      "name": "Cursor",
      "enabled": true,
      "outputPath": "a",
      "outputType": "directory",
      "fileExtensions": [],
      "organizeBySource": true,
      "generateIndex": false,
      "indexFileName": "index.md",
      "preserveDirectoryStructure": false,
      "useOriginalFilename": true,
      "isRuleType": true
    }
  ]
}
